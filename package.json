  },
  "homepage": "https://github.com/bytesized/NextComic#readme",
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "web-ext": "^3.2.1"
  },
  "dependencies": {}
